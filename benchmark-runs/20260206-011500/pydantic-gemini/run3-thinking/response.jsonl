{"type": "TOOL_CALL_START", "toolCallId": "pyd_ai_f0e81c36011544d4aad3c50d8f122b04", "toolCallName": "calculator", "parentMessageId": "3b87c764-88e3-4ce5-81fb-8a3cb6d49bf1"}
{"type": "TOOL_CALL_ARGS", "toolCallId": "pyd_ai_f0e81c36011544d4aad3c50d8f122b04", "delta": "{\"expression\":\"12 - 5\"}"}
{"type": "TOOL_CALL_END", "toolCallId": "pyd_ai_f0e81c36011544d4aad3c50d8f122b04"}
{"type": "TOOL_CALL_RESULT", "messageId": "f4363e4a-332f-4fbb-b9fd-1bdac794dfb6", "toolCallId": "pyd_ai_f0e81c36011544d4aad3c50d8f122b04", "content": "12 - 5 = 7", "role": "tool"}
{"type": "TEXT_MESSAGE_START", "messageId": "1d3d3415-7086-492e-91b9-fb7e737fa6fd", "role": "assistant"}
{"type": "TEXT_MESSAGE_CONTENT", "messageId": "1d3d3415-7086-492e-91b9-fb7e737fa6fd", "delta": "Therefore, x ="}
